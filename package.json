{
	"name": "autonest",
	"version": "1.0.0",
	"description": "Convert nested data structures to FormData and back",
	"scripts": {
		"build:browser": "rollup -c",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"lint": "eslint .",
		"prepublishOnly": "npm test && npm run lint"
	},
	"jest": {
		"testEnvironment": "jsdom"
	},
	"exports": {
		".": {
			"browser": "./src/browser.js",
			"default": "./src/server.js"
		}
	},
	"type": "module",
	"keywords": [
		"formdata",
		"nested",
		"data structure",
		"serialization",
		"deserialization",
		"javascript",
		"utility"
	],
	"author": "Michael Medvedev <mmedvedev20@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/autonest.git"
	},
	"bugs": {
		"url": "https://github.com/yourusername/autonest/issues"
	},
	"homepage": "https://github.com/yourusername/autonest#readme",
	"devDependencies": {
		"@eslint/js": "^9.14.0",
		"@rollup/plugin-commonjs": "^28.0.1",
		"@rollup/plugin-node-resolve": "^15.3.0",
		"eslint": "^8.57.1",
		"globals": "^15.12.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"rollup": "^4.25.0"
	}
}
